{
  "mcpServers": {
    "graphite": {
      "command": "gt",
      "args": ["mcp"]
    },
    "serena": {
      "command": "uvx",
      "args": ["--from", "serena-mcp", "serena-mcp"]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": ["-y", "@anthropic/mcp-sequentialthinking"]
    },
    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"]
    },
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"],
      "env": {
        "CONTEXT7_API_KEY": "${CONTEXT7_API_KEY}"
      }
    }
  }
}
